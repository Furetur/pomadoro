{
  "name": "pomadoro-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "xo": {
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true
          }
        }
      ],
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "**/*.css"
          ]
        }
      ]
    },
    "extends": [
      "xo/browser",
      "xo-react"
    ],
    "prettier": true
  },
  "scripts": {
    "start": "parcel public/index.html",
    "test": "xo"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@fseehawer/react-circular-slider": "^2.3.3",
    "antd": "^4.4.2",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "react": "^16.13.1",
    "react-circular-slider": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/react": "^16.9.41",
    "@types/react-select": "^3.0.14",
    "@types/styled-components": "^5.1.1",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.6",
    "xo": "^0.32.1"
  }
}
